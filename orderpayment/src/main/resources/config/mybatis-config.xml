<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="logImpl" value="SLF4J" />
	</settings>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${datasource.driver}" />
				<property name="url" value="${datasource.url}" />
				<property name="username" value="${datasource.username}" />
				<property name="password" value="${datasource.password}" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="mapper/ProductInfoMapper.xml" />
		<mapper resource="mapper/OrderInfoMapper.xml" />
		<mapper resource="mapper/AppInfoMapper.xml" />
		<mapper resource="mapper/RechargeCardInfoMapper.xml" />
		<mapper resource="mapper/BraintreePlanInfoMapper.xml" />
		<!--
		<mapper resource="mapper/AuthInfoMapper.xml" />
		<mapper resource="mapper/SKUInfoMapper.xml" />		
		<mapper resource="mapper/SKUTypeInfoMapper.xml" />
		<mapper resource="mapper/PolicyMapMapper.xml" />
		<mapper resource="mapper/RegionMapMapper.xml" />
		<mapper resource="mapper/UserInfoMapper.xml" />
		-->
	</mappers>
</configuration>